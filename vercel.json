{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/recarga",
      "dest": "/recarga/index.html"
    },
    {
      "src": "/recarga/",
      "dest": "/recarga/index.html"
    },
    {
      "src": "/recarga/indexa05a.html",
      "dest": "/recarga/indexa05a.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
